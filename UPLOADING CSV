		$handle = fopen($file,"r");
		while (($fileop = fgetcsv($handle,1000,","))!== false){

			$name=$fileop[0];
			$date=$fileop[1];

			$sql = mysql_query ("INSERT INTO data_123 (`Name`,`Date`) VALUES('$name','$date')");
		}
		header('location:displaydata.php');
	}
	?>
